{
    "server": {
        "ip": "127.0.0.1",
        "port": 8080
    },
    "monitors":[
        {
            "name":"Process monitoring-agent-daemon",
            "schedule": "0 0 0 0 0 0",
            "details": {
                "type": "process",
                "applicationNames": ["app1", "app2"],
                "thresholdMemWarn": 100,
                "thresholdMemError": 100,
                "storeValues": true
            }
        }
    ]
}